{
  "name": "lodash-snippets",
  "displayName": "Lodash Snippets",
  "description": "Snippets for popular lodash functions.",
  "version": "0.7.0",
  "publisher": "tomphilbin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomphilbin/lodash-snippets.git"
  },
  "engines": {
    "vscode": "^0.10.10"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "Javascript",
    "Typescript",
    "Lo-Dash",
    "lodash",
    "snippets"
  ],
  "icon": "lodash-icon.png",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./javascript.json"
      },
      {
        "language": "typescript",
        "path": "./javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./javascript.json"
      }
    ]
  },
  "scripts": {
    "build": "babel src/ -d build/",
    "create-jsdoc": "jsdoc node_modules/lodash -X > jsdoc-output.json",
    "lint": "eslint src/",
    "start": "npm run build && npm run create-jsdoc && node build/index.js",
    "test": "mocha test/specs --require test/helpers/mocha-chai.helper.js --compilers js:babel-core/register",
    "tdd": "npm run test -- -w",
    "vscode:prepublish": "npm start"
  },
  "dependencies": {
    "lodash": "^4.16.4",
    "jsdoc": "^3.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.3.0",
    "mocha": "^3.1.0"
  }
}
